        	const url = new URL(location.href
            	+'/data'
                +'?TARGET_TEMP='+ document.getElementById('target_temp').value
                +'&TARGET_CO2='+ document.getElementById('target_co2').value
                +'&TARGET_TEMP='+ document.getElementById('switch').checked);
            
               console.log('LOG:'+url);
           	alert (url);

